<div class="row">
  <div class="col-md-10 col-md-offset-1"><h2>Feedback </h2></div>
  <div class="col-md-4 col-md-offset-1">
    <div class="panel panel-default">
      <div class="panel-heading">
        <div class="col-md-6">Displaying Comments</div>
          <div class="col-md-6 text-right">
          {{#link-to 'addNewComment' class="btn btn-default"}}<i class="fa fa-plus"></i> New Comment{{/link-to}}
          </div>
        </div>
        <div class="panel-body">
          <div class="text-center">
            <ul class="pagination">
              <li class="disabled"><a href="#"><span aria-hidden="true">«</span><span class="sr-only">Previous</span></a></li>
              <li class="active"><a href="#">1 <span class="sr-only">(current)</span></a></li>
              <li><a href="#">2</a></li>
              <li><a href="#">3</a></li>
              <li><a href="#">4</a></li>
              <li><a href="#">5</a></li>
              <li><a href="#"><span aria-hidden="true">»</span><span class="sr-only">Next</span></a></li>
           </ul>
         </div>
          <ul class="list-group">
            {{#each model}}
              {{#link-to 'comment' this class="list-group-item"}}
              
                <div class="row">
                  <div class="col-md-6">
                    <label>{{userName}}</label>
                    <div class="description">
                    {{description}}
                    </div>
                  </div>
                  <div class="col-md-6 text-right">
                    {{createDate}}
                    <i class="fa fa-chevron-right"></i>

                  </div>
                </div>
              {{/link-to}}
            {{/each}}
          </ul>
        </div>
    </div>
  </div><!-- //col-md-4 col-md-offset-2 -->

  <div class="col-md-4">{{outlet}}</div>
 
  </div><!-- //col-md-4 -->

</div>
